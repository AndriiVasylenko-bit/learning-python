<xs:element name="person" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:complexType> <!--складний тип-->
        <xs:sequence> <!--послідовність-->
            <xs:element name="full_name" type="xs:string"
                        minOccurs="1" maxOccurs="1"/>
            <xs:element name="child_name" type="xs:string"
                        minOccurs="0" maxOccurs="10"/>
        </xs:sequence>
    </xs:complexType>
</xs:element>
